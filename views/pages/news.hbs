<header class="news-header">
    <div class="row align-items-center justify-content-center">
        <div class="col-lg-5 header-left">
            {{#each headerLeft}}
            <h1>{{text}}</h1>
            {{/each}}
        </div>
        <div class="col-lg-5 header-right">
            {{#each headerRight}}
            <img src="/images/{{img}}" alt="">
            {{/each}}
        </div>
    </div>
</header>
<section class="news-section">
    <div class="news-section_top">
        <h1>Eng so‘ngi yangiliklar</h1>
        <div class="row">
            <div class="col-lg-8">
                <div class="block">
                    {{#each newsTop}}
                    <img src="/images/{{img}}" alt="" class="active-img">
                    <div class="more">
                        <div class="more_top">
                            <div class="col-lg-4 left">
                                <div class="user-img">
                                    <img class="imgggg" src="../images/logo.png" alt="">
                                </div>
                                <div class="user-name">
                                    <h4>ChangeIt-Academy</h4>
                                    <h5>Meneger</h5>
                                </div>
                            </div>
                            <div class="col-lg-4 right">
                                <p>{{data}}</p>
                            </div>
                        </div>
                        <div class="col-lg-12 more-bottom">
                            <h2>{{title}}</h2>
                            <h5>{{more}}</h5>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div class="col-lg-4">
                {{#each news}}
                <div class="card-right mt-2">
                    <div class="row newsClick" data-id="{{id}}">
                        <div class="col-lg-5">
                            <img src="/images/{{img}}" alt="">
                        </div>
                        <div class="col-lg-7">
                            <h3>{{title}}</h3>
                            <h4 class="data">{{data}}</h4>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
