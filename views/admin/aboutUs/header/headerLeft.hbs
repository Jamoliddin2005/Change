<div class="content">
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <div class="card card-primary">
                    <form action="/admin/aboutUs/header-left" method="POST" enctype="multipart/form-data">
                        <label for="header-left-img">Header-left img</label>
                        <input type="file" class="form-control" id="header-left-img" name="img" required />
                        <button class="btn btn-primary">Submit</button>
                    </form>
                </div>
            </div>
            <div class="col-lg-12 left-header admin-header-left pt-5">
                <div class="row">
                    {{#each header}}
                    <div class="col-lg-2">
                        <img src="/images/{{img}}" alt="" />
                        <div class="row pt-3 col-lg-10">
                            <a href="/admin/aboutUs/header-left-img/{{_id}}" class="btn btn-primary">Delete</a>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>